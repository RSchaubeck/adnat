<h1>Adnat</h1>
<h3>Log In</h3>

<div class="session-form">
    <%= form_with url: login_path do |f| %>
        <%= f.label :email, "Email" %>
        <%= f.text_field :email %>
        <%= f.label :password, "Password" %>
        <%= f.password_field :password %>
        <%= f.submit "Log In" %>
    <% end %>
    <%= link_to 'Sign Up', signup_path %>
</div>